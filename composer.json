{
    "name": "ajthinking/git",
    "description": "A PHP git repository control library for Laravel",
    "keywords": [
        "ajthinking",
        "git",
		"php",
		"laravel"
    ],
    "homepage": "https://github.com/ajthinking/git",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Anders JÃ¼risoo",
            "email": "jurisoo@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
		"illuminate/support": "^8.0"		
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",		
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ajthinking\\Git\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ajthinking\\Git\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ajthinking\\Git\\GitServiceProvider"
            ],
            "aliases": {
                "Git": "Ajthinking\\Git\\GitFacade"
            }
        }
    }
}
